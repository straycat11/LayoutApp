<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView9"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_width="0dp"
            android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/imageView12" android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="24dp"/>
    <TextView
            android:text="LayoutApp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView9" app:layout_constraintStart_toEndOf="@+id/imageView12"
            android:layout_marginStart="40dp" android:textSize="18sp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView25"
            app:layout_constraintStart_toEndOf="@+id/textView4" app:layout_constraintHorizontal_bias="0.88"
            app:layout_constraintEnd_toStartOf="@+id/imageView26" app:layout_constraintTop_toTopOf="@+id/imageView26"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26"
            app:layout_constraintHorizontal_chainStyle="packed" app:layout_constraintVertical_bias="1.0"
    />
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView26"
            app:layout_constraintStart_toEndOf="@+id/imageView25" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView27" app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27" app:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="32dp" android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            tools:layout_editor_absoluteY="145dp" android:id="@+id/imageView27"
            app:layout_constraintStart_toEndOf="@+id/imageView26" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView32"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView33" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView12" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginEnd="8dp" android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView33"
            app:layout_constraintStart_toEndOf="@+id/imageView32" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView34" app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView34"
            app:layout_constraintStart_toEndOf="@+id/imageView33" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView35" app:layout_constraintTop_toTopOf="@+id/imageView33"
            app:layout_constraintBottom_toBottomOf="@+id/imageView33" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginEnd="8dp" android:layout_marginStart="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView35"
            app:layout_constraintStart_toEndOf="@+id/imageView34" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="@+id/imageView34"
            app:layout_constraintDimensionRatio="1:1" android:scaleType="centerCrop" android:layout_marginEnd="16dp"
            android:layout_marginStart="8dp"/>
</android.support.constraint.ConstraintLayout>